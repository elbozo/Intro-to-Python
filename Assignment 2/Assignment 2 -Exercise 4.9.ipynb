    "def fahrenheit(c):\n",
    "    f = (9/5)*c + 32\n",
    "    return f\n",
    "\n",
    "def display_table():\n",
    "    print('Celcius','\\t','Fahrenheit')\n",
    "    for x in range(0,101):\n",
    "        print('{:>7}\\t\\t{:>11}'.format(x,round(fahrenheit(x),2)))\n",
    "    \n",
    "display_table()"
   ]
